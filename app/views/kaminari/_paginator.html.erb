<%= paginator.render do %>
  <br>
  <ul class="pagination center-align">
    <%= first_page_tag %>
    <% #= prev_page_tag unless current_page.first? %>
    <% each_page do |page| %>
      <% if page.display_tag? %>
        <%= page_tag page %>
      <% elsif !page.was_truncated? %>
        <%= gap_tag %>
      <% end %>
    <% end %>
    <% #= next_page_tag unless current_page.last? %>
    <%= last_page_tag %>
  </ul>
<% end %>
